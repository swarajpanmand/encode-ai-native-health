{
  "expo": {
    "name": "client-mobile",
    "slug": "client-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "clientmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to scan documents and extract text.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select images for text extraction."
      },
      "bundleIdentifier": "com.swarajpanmand.clientmobile"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.swarajpanmand.clientmobile",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ed4ab48a-74f1-4a63-a541-52d4d79e3f67"
      }
    }
  }
}